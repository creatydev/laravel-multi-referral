{
    "name": "devi/laravel-multi-referral",
    "description": "A Multi referral System With Laravel",
    "keywords": ["referral", "referral-system", "affiliate-tracking","multi-referral"],
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "devi",
            "email": "devserhii@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "laravel/laravel": "^5.5",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Devi\\MultiReferral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devi\\MultiReferral\\MultiReferralServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
